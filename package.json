{
  "name": "pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run lint -- --fix && vue-cli-service serve",
    "build": "npm run lint -- --fix && vue-cli-service build",
    "refresh": "node --input-type=module -e \"import generateSiteDictionary from './src/node/generateSiteDictionary.js'; generateSiteDictionary();\"",
    "generate": "npm run lint -- --fix && vue-cli-service build && node --input-type=module -e \"import prerender from './src/node/prerender.js'; prerender();\"",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@prerenderer/prerenderer": "^0.7.2",
    "@prerenderer/renderer-jsdom": "^0.2.0",
    "@prerenderer/renderer-puppeteer": "^0.2.0",
    "core-js": "^3.6.5",
    "directory-tree": "^2.2.4",
    "front-matter": "^3.2.1",
    "fs-extra": "^8.1.0",
    "mkdirp": "^0.5.5",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-sidebar-menu": "^4.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "frontmatter-markdown-loader": "^3.4.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
